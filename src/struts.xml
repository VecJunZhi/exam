<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
	"http://struts.apache.org/dtds/struts-2.3.dtd">

<struts>
	<constant name="struts.i18n.encoding" value="UTF-8" />
    <constant name="struts.enable.DynamicMethodInvocation" value="false" />
    <constant name="struts.devMode" value="true" />

    <package name="default" extends="json-default">
        <action name="HelloJsonAction_*" class="cn.gov.scciq.bussiness.example.HelloJsonAction" method="{1}">
             <result type="json">
                 <param name="root">result</param>
             </result>
        </action>
      	<action name="SelectDataAction_*" class="cn.gov.scciq.bussiness.select.SelectDataAction" method="{1}">
             <result type="json">
                 <param name="root">result</param>
             </result>
        </action>
      	<action name="SearchSelectAction_*" class="cn.gov.scciq.bussiness.searchSelect.SearchSelectAction" method="{1}">
             <result type="json">
                 <param name="root">result</param>
             </result>
        </action>
        <action name="ConventionCtrlAction_*" class="cn.gov.scciq.bussiness.riskCtrl.ConventionCtrlAction" method="{1}">
             <result type="json">
                 <param name="root">result</param>
             </result>
        </action>
        <action name="CIQCtrlAction_*" class="cn.gov.scciq.bussiness.ciqCtrl.CIQCtrlAction" method="{1}">
             <result type="json">
                 <param name="root">result</param>
             </result>
        </action>
    </package>

</struts>
